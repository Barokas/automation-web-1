    "babel-preset-es2015": "~6.14.0",
    "babel-register": "~6.14.0",
    "chai": "~3.5.0",
    "cucumber": "1.3.1",
    "json-query": "^2.2.0",
    "logs-to-sumologic": "^1.0.2",
    "newrelic-winston": "0.0.1",
    "restler": "^3.4.0",
    "sentence-generator": "^1.0.1",
    "sync-request": "^3.0.1",
    "wdio-cucumber-framework": "^0.2.13",
    "wdio-phantomjs-service": "~0.1.0",
    "wdio-selenium-standalone-service": "~0.0.6",
    "wdio-spec-reporter": "~0.0.3",
    "webdriverio": "^4.5.0"
