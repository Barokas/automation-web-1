  "description": "Boilerplate project to run WebdriverIO tests with Cucumber",
  "homepage": "https://github.com/webdriverio/cucumber-boilerplate",
  "main": "test/runner.js",
  "scripts": {
    "post-task": "./wdio --spec test/features/post_task_as_guest.feature wdio.stage.conf.js",
    "post-ikea-task": "./wdio --spec test/features/post_ikea_task_as_guest.feature wdio-eyes.stage.conf.js",
    "full-feature": "./wdio --spec test/features-multiremote/post_and_claim_task_multi.feature wdio.multiremote.stage.conf.js",
    "full-feature-eyes": "./wdio --spec test/features-multiremote/post_and_claim_task_multi.feature wdio-eyes.multi.stage.conf.js",
    "landing-pages": "./wdio --spec test/features/landing_pages_cta.feature wdio.stage.conf.js",
    "eyes-demo": "./wdio --spec test/features/eyes_demo.feature wdio-eyes.stage.conf.js",
	"eyes-regression": "./wdio --spec test/features/eyes_regression.feature wdio-eyes.stage.conf.js",
	"eyes-multi": "./wdio --spec test/features/eyes_regression_multi.feature wdio-eyes.multi.stage.conf.js"
