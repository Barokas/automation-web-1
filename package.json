    "selenium-standalone": "~5.6.2",
    "selenium-webdriver": "^3.0.0-beta-3",
    "wdio-allure-reporter": "~0.0.2",
    "wdio-junit-reporter": "^0.1.0",
    "wdio-mocha-framework": "^0.5.2",
    "wdio-sauce-service": "^0.2.5",
    "wdio-screenshot": "^0.3.0",
    "winston": "^2.2.0",
    "winston-annotate": "^0.1.0"
